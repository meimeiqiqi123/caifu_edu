.page{
  height:100%;
  width: 100%;
  overflow: hidden
}
.cover{
  width: 100%;
  height: 100%;
}
.comment{
  position: fixed;
  top:5%;
  left:20%;
  height:50rpx;
  width: 460rpx;
  background-color: rgba(105,105,105,0.4);
  display: flex
}
.comment .title{
  font-size: 30rpx;
  color:#fff

}
.comment .com_swp{
  width: 360rpx;
}
.comment .toux image{
 width: 50rpx;
  height: 50rpx;
}
.comment .mine{
  font-size: 24rpx;
  color: #fff;
  background-color: #48D1CC;
  width: 100rpx;
  height: 46rpx;
  line-height:46rpx; 
  text-align: center;
  margin-top: 2rpx
}
.right_content{
   position: fixed;
  top:40%;
  right:0;
}
.music{
  position: fixed;
  top:20%;
  left:3%;
  width: 60rpx;
  height: 60rpx;
   border-radius: 50%;
  border: 1rpx solid #fff
}
.music .avatar{
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
  margin-left: 5rpx
}

.zan{
  margin-top: 40rpx;
}
.zan .avatar{
  width: 80rpx;
  height: 80rpx;
  margin-right: 15rpx;
}
.zan .zan_title{
  color: #fff;
  font-size: 26rpx;
  text-align: center;
  margin-right: 15rpx;
  margin-top: 0
}

.zan_button{
  width: 50rpx;
  height: 50rpx;
background-color:transparent;
background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%
}
.zan_button::after{
  border: none
}

.comment-bottom{
  position: absolute;
  left:0;
  bottom: 0;
  width:100%;
  background:#fff;
  z-index: 99;

}
.send-comment{
  width:100%;
  height: 86rpx;
  margin: 5rpx;
  border-top: 1rpx solid #e1e1e1;
}

.send-view{
  display: inline-block;
  vertical-align: top;
  margin: 15rpx 15rpx 15rpx 55rpx;
}

.send-buttom{
  display: inline-block;
  vertical-align: top;
  margin: 18rpx 22rpx 18rpx 18rpx
}
.send-text{

  font-size:34rpx;

  font-weight:320;

  color:#e1e1e1;

}
.activty{
  color: #0099Ff

}
.send-input{
  width:530rpx;
  min-height: 65rpx;
  max-height: 86rpx;
  background: #e1e1e1;
  border-radius: 30rpx;
  outline: none;
  overflow: hidden;
  padding-left: 20rpx;
  margin-bottom: 24rpx;

}


.left_content{
  position: fixed;
  bottom:10%;
  left:0;
}
.left_content .agency{
  margin-left: 30rpx;
  color: #fff;
  font-size: 30rpx
}
.left_content .nickname{
  margin-left: 30rpx;
  margin-top: 10rpx;
  color: #0099FF;
  font-size: 26rpx
}

.foot_tab_b{
  width: 100%;
