}

.scroll-view_H{
    white-space: nowrap;
}
.scroll-view-item_H{
    display: inline-block;
    width: 180rpx;
    height: 200rpx;
    margin-top: 20rpx;
    text-align: center;
    padding-right: 20rpx
    
}
.scroll-view-item_H .item_head{
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%
}

.scroll-view-item_H .title_head{
    margin-top: 0
} 
.scroll-view-item_H .page{
    margin-top: 10rpx;
    color: #5060b5
} 
.price_div{
  width: 100%;
  height: 100rpx;
  background: #f8f8f8;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-between; 
}
.kefu{
  flex: 20%;
  font-size: 24rpx;
  margin-left: 50rpx;
  margin-top: 30rpx
}

.price_btn{
  flex: 80%;
  margin:10rpx auto
}
.price_button{
  width: 200rpx;
  height: 90rpx;
  background: #FFC93A;
  text-align: center;
  color:#000;
  font-size: 14px;
  border-radius: 10rpx;
  margin-left: 100rpx;
  margin-right: 10rpx;
  float:right
}
.price_button::after{ border: none; }

