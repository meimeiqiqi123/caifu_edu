page{
  background-color: #d9eeff
}

.course_list{
  width: 95%;
  margin: 10rpx auto
}
.course_list .course{
  margin-top: 20rpx;
  width: 100%;
  height: 280rpx;
  display: flex;
  border-radius: 20rpx;
  box-shadow: 0 3px 6px 0 #237bc2;
}
.course_list .course image{
  flex:60%;
  height: 280rpx;
  border-top-left-radius: 20rpx;
  border-bottom-left-radius:20rpx;
 
} 
.course_list .course .content{
  flex:40%;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  padding: 10rpx;
  border-top-right-radius: 20rpx;
  border-bottom-right-radius:20rpx;
}
.content .a_name{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 30rpx;
}
.content .a_name .name{
  overflow: hidden;
 text-overflow: ellipsis;
  white-space: nowrap;
  width: 260rpx
}
.content .a_name image{
  width: 35rpx;
  height: 30rpx;
  margin-right: 5rpx;
}
.content .c_name{
  font-size: 24rpx;
  margin-top: 10rpx;
}
.content .tuan{
  font-size: 24rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}
.content .c_price{
  font-size: 28rpx;
  color: red;
  margin-top: 10rpx
}
.content .b_b{
text-align: center;
margin-top: 10rpx;
}
.content .b_b image{
  width: 180rpx;
  height: 50rpx;
}
.nocontent{
  text-align: center;
  font-size: 28rpx;
}
.course_title{
  width: 100%
}
.course_title .title_img{
  text-align: center;
  margin: 20rpx auto
}
.course_title .title_img .ct{
 width:480rpx;
 height: 100rpx;
}
.course_title .gc{
 width:220rpx;
 height: 60rpx;
 position: absolute;
 margin-top: 20rpx;
 margin-left: 45%

}
