}


.address{
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle
}
.phone{
  margin-top: 30rpx;
  margin-bottom: 100rpx;
  padding-bottom: 20rpx
}

.shade{
  width: 100%;
  height: 100%;
  position: fixed;
  background: #333;
  z-index:77;
  top:0;
  left: 0;
  opacity: 0.8;
}
.room{
  width: 60%; height: 50%;
  top:25%;
  left: 20%;
  position:fixed;
  z-index:999;
  background: white;
  border-radius: 10rpx;
  
}
.room_div{
  position:relative;
  font-size: 26rpx;
  width:80%;
  margin: 0 auto
}
.close{
  position: absolute;
  top:10rpx;
  right: 0rpx;
  width: 70rpx;
  height: 70rpx;
}
.close image{
  width: 50rpx;
  height: 50rpx;
}
.secret_title{
  height: 90rpx;
  line-height: 90rpx;
