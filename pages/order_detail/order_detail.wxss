}
.share .share_list{
  display: flex;
  flex-direction: row;
  justify-content: space-between
}
.share .share_list .share_b{
  width: 160rpx;
  height: 60rpx;
  border-radius: 30rpx;
  color: #fff;
  font-size: 26rpx;
  line-height: 60rpx;
  text-align: center
}
.share .share_list .share_img{
  display: flex;
  padding-right: 10rpx;
  margin-top: 10rpx
}
.share .share_list .share_img .item{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
