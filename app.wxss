/** Global Border
 *==============================================================================*/
.b-none {
  border: none;
}
.b-none:after,
.b-none:before {
  display: none;
}
.bl,
.br,
.bt,
.bb {
  position: relative;
}
.bt-none:before,
.bl-none:before,
.bb-none:after,
.br-none:after {
  display: none;
}
.bt:before {
  top: 0;
  border-top: 1px solid #e0e3e3;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
}
.bb:after {
  bottom: 0;
  border-bottom: 1px solid #e0e3e3;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
}
.bt:before,
.bb:after {
  content: " ";
  position: absolute;
  left: 0;
  width: 100%;
  height: 1px;
  color: #d9d9d9;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.bl:before {
  left: 0;
  border-left: 1px solid #e0e3e3;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
}
.br:after {
  right: 0;
  border-right: 1px solid #e0e3e3;
  -webkit-transform-origin: 100% 0;
  transform-origin: 100% 0;
}
.bl:before,
.br:after {
  content: "";
  position: absolute;
  top: 0;
  width: 1px;
  height: 100%;
  color: #d9d9d9;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
}
